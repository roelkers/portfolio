{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "Blog"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "text": {
      "type": "string"
    },
    "content": {
      "type": "richtext"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "slug": {
      "type": "uid",
      "targetField": "text"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "SQL",
        "React",
        "Javascript",
        "Nodejs"
      ]
    },
    "date": {
      "type": "date"
    }
  }
}
